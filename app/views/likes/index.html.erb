<div style="width:450px;height:270px;margin-left:auto;margin-right:auto;margin-top:50px">
<div style="width:200px;height:50px; background: #333;text-align:center">
<h1 style="font-family:verdana;font-size:14px;color:white;padding-top:15px;">Denys Bastov</h1>
</div>
<div style="width:250px; height:50px; position:absolute; margin-left:200px;background:#666;margin-top:-50px;box-shadow: 3px 0px 10px rgba(0,0,0,0.5) inset"> 
<a href="https://github.com/bastovd/proj1" style="line-height:45px;margin-left:20px;font-family:verdana;font-size:12px;color:white">https://github.com/bastovd/proj1</a>
</div>
<img src="http://poroawards.net/wp-content/themes/PATheme/images/site/porologo.png" style="width:200px; box-shadow: 0px 5px 10px rgba(0,0,0,0.5)">
<div style="width:250px;height:220px; background: #fff;text-align:center;position:relative;margin-left:200px;margin-top:-225px;box-shadow: 5px 0px 10px rgba(0,0,0,0.5) inset">
<table style="width:250px;">
<tr>
	<td >title</td>
	<td >user</td>
</tr>
<% @likes.each do |liked| %>
<tr>
	<td><%= liked.title %></td>
	<td><%= liked.user.name %></td>
	<td><%= link_to "show", like_path(liked.id) %> </td>
	<td><%= link_to "edit", edit_like_path(liked.id) %> </td>
	<td><%= button_to "destroy", liked.id, confirm: "you sure you want to do this?", method: :delete %> </td>
</tr>
<% end %>

</table>
<div style="width:250px;height:30px;background:#222;font-family:verdana; font-size:14px;text-decoration:none;">
	<a href="<%= likes_path %>/new"><p style="color:white;margin-top:20px;padding-top:5px"><span style="height:30px">Add new like</span></p></a>
</div>
</div>
</div>