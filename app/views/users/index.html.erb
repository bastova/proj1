<table style="width:250px;">
	<tr style="text-align:left">
		<td>name</td>
		<td>email</td>
		<td>password</td>
	</tr>
	<% @users.each do |user| %>
	<tr style="text-align:left">
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to "show", user_path(user.id) %> </td>
		<td><%= link_to "edit", edit_user_path(user.id) %> </td>
		<td><%= link_to "destroy", user, confirm: "you sure you want to do this?", method: :delete %> </td>
	</tr>
	<% end %>
</table>
<div style="width:250px;height:30px;background:#222;font-family:verdana; font-size:14px;text-decoration:none;">
	<a href="<%= users_path %>/new"><p style="color:white;margin-top:20px;padding-top:5px"><span style="height:30px">Add new user</span></p></a>
